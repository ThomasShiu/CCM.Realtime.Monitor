    <script>

        function btn_resetpassword() {
            var keyValue = '@CCM.Code.OperatorProvider.Provider.GetCurrent().UserId';
            var Account = '@CCM.Code.OperatorProvider.Provider.GetCurrent().UserCode';
            var RealName = '@CCM.Code.OperatorProvider.Provider.GetCurrent().UserName';
            $.modalOpen({
                id: "RevisePassword",
                title: '修改密碼',
                url: '/SystemManage/User/ResetPassword?keyValue=' + keyValue + "&account=" + escape(Account) + '&realName=' + escape(RealName),
                width: "450px",
                height: "320px",
                callBack: function (iframeId) {
                    top.frames[iframeId].submitForm();
                }
            });
        }


        // My own arbitrary use of isMobile, as an example
        (function () {
            var MOBILE_SITE = '/m/mobile/Index', // site to redirect to
                NO_REDIRECT = 'noredirect'; // cookie to prevent redirect

            // I only want to redirect iPhones, Android phones and a handful of 7" devices
            if (isMobile.apple.phone || isMobile.android.phone || isMobile.seven_inch) {

                // Only redirect if the user didn't previously choose
                // to explicitly view the full site. This is validated
                // by checking if a "noredirect" cookie exists
                if (document.cookie.indexOf(NO_REDIRECT) === -1) {
                    document.location = MOBILE_SITE;
                }
            }
        })();


        $(document).ready(function () {
            //每十分鐘執行，無限次，並命名計時器名稱為 TimerRefresh 36hs
            //若時間間隔抵到，但函式程序仍未完成則需等待執行函式完成後再繼續計時
            $('body').everyTime('600s', 'TimerRefresh', function () {
                location.reload();
                return false;
                //$("#gridList").trigger("reloadGrid");

            }, 0, true);
        });
    </script>
